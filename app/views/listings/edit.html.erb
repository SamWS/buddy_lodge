  <div class="housePetFormPage">
    
    <div class="row">
    <h3>The House</h3>

       <%= form_tag '/listings', class: 'col s12',method: 'post' do %>

        <div class="row">
          <div class="input-field col s6">
            <i class="material-icons prefix">location_on</i>
            <input id="autocomplete1" placeholder="Enter your address"
             onFocus="geolocate()" type="text" name="address">
          </div>
          <div class="input-field col s6">
            <i class="material-icons prefix">perm_contact_calendar</i>
            <input id="icon_telephone" type="tel" class="validate" name="date">
            <label for="date">Dates</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">store</i>
            <input id="icon_prefix" type="text" class="validate" name="image">
            <label for="image">House Photo</label>
          </div>
        </div>
      <h3>The Pet</h3>
        <div class="row">
          <div class="input-field col s6">
            <i class="material-icons prefix">grade</i>
            <input id="icon_prefix" type="text" class="validate" name="name">
            <label for="name">Name</label>
          </div>
          <div class="input-field col s6">
            <i class="material-icons prefix">android</i>
            <input id="icon_telephone" type="tel" class="validate" name="species">
            <label for="species">Species</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <i class="material-icons prefix">label</i>
            <input id="icon_prefix" type="text" class="validate" name="age">
            <label for="age">Age</label>
          </div>
          <div class="input-field col s6">
            <i class="material-icons prefix">perm_media</i>
            <input id="icon_telephone" type="tel" class="validate" name="avatar">
            <label for="avatar">Photo</label>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <textarea id="textarea1" class="materialize-textarea" length="120" name="describe"></textarea>
            <label for="describe">Describe your House and Pet</label>
          </div>
        </div>
        <button>submit</button>
      <% end %>
    </div>
    
  </div>